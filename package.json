{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "description": "React Application Boilerplate",
  "typings": "dist/index",
  "scripts": {
    "analyze:dev": "webpack --mode=development --analyze --config ./webpack.config.js",
    "analyze:prod": "webpack --mode=production --analyze --config ./webpack.config.js",
    "build": "webpack --mode=production --config ./webpack.config.js",
    "eslint:fix": "node ./node_modules/eslint/bin/eslint.js ./src/**/*.{js,ts,tsx} --fix",
    "eslint": "node ./node_modules/eslint/bin/eslint.js ./src/**/*.{js,ts,tsx}",
    "start": "npx webpack-dev-server --mode=development --colors",
    "stylelint:fix": "stylelint \"src/**/*.css\" --config=configs/stylelint.config.js --fix",
    "stylelint": "stylelint \"src/**/*.css\" --config=configs/stylelint.config.js  --cache",
    "test-coverage": "jest --coverage --watchAll=false --config=jest.config.js",
    "test:watch": "npm test -- --watchAll",
    "test": "jest --config=jest.config.js --watchAll=false"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "ie >= 11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint"
    ],
    "*.{css}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "axios": "0.21.1",
    "classnames": "2.2.6",
    "core-js": "3.8.2",
    "history": "4.10.1",
    "jest": "26.6.3",
    "prop-types": "15.7.2",
    "qs": "6.9.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typescript": "4.1.3"
  },
  "devDependencies": {
